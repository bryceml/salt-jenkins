
def mine = 'mine1'
def mine3 = 'mine3'

stage('run') {
  node('kitchen-slave') {
    sh '''
    export mine=mine2
    echo $mine
    echo ${mine}
    echo $mine3
    echo ${mine3}
    git ls-remote https://github.com/saltstack/salt-jenkins.git|grep refs/pull/${CHANGE_ID}/head$ || true
    '''
    sh """
    export mine=mine2
    echo $mine
    echo ${mine}
    echo \$mine
    echo \${mine}
    echo $mine3
    echo ${mine3}
    echo ${env.mine}
    echo $env.mine
    """
  }
}
