
def mine = 'mine1'
def mine3 = 'mine3'

stage('run') {
  node('kitchen-slave') {
    sh '''
    export mine=mine2
    echo $mine
    echo ${mine}
    echo $mine3
    echo ${mine3}
    '''
    sh """
    export mine=mine2
    echo $mine
    echo ${mine}
    echo $mine3
    echo ${mine3}
    echo ${env.mine}
    echo $env.mine
    """
  }
}
